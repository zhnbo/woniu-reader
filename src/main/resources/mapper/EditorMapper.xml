<mapper namespace="com.woniuxy.reader.mapper.EditorMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.woniuxy.reader.entity.Editor">
        <id column="editor_id" property="editorId" />
        <result column="editor_tel" property="editorTel" />
        <result column="password" property="password" />
