<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.woniuxy.reader.mapper.BookDataMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.woniuxy.reader.entity.BookData">
        <id column="book_data_id" property="bookDataId" />
        <result column="book_id" property="bookId" />
        <result column="click_number" property="clickNumber" />
        <result column="collection" property="collection" />
        <result column="subscribe" property="subscribe" />
